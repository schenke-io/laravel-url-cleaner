{
    "name": "schenke-io/laravel-url-cleaner",
    "description": "check and cleans url from seo or tracking data",
    "type": "library",
    "keywords": [
        "schenke-io"
    ],
    "homepage": "https://github.com/schenke-io/laravel-url-cleaner",
    "license": "MIT",
    "authors": [
        {
            "name": "K.-U. Schenke"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "archtechx/enums": "^1.1",
        "guzzlehttp/guzzle": "^7.9",
        "spatie/laravel-package-tools": "^1.30"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "laravel/framework": "^12.40",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^10.9",
        "pestphp/pest": "^4.0",
        "larastan/larastan": "^v3.9",
        "spatie/ray": "^1.40",
        "schenke-io/packaging-tools": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "SchenkeIo\\LaravelUrlCleaner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SchenkeIo\\LaravelUrlCleaner\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "docs": "vendor/bin/testbench make:readme",
        "data": "vendor/bin/testbench make:data",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "coverage": "vendor/bin/pest --coverage",
        "pint": "vendor/bin/pint",
        "low": "composer update --prefer-lowest --prefer-dist",
        "high": "composer update --prefer-stable --prefer-dist",
        "check": [
            "@data",
            "@docs",
            "@pint",
            "@analyse",
            "@coverage"
        ],
        "release": [
            "@data",
            "@docs",
            "@pint",
            "@low",
            "@test",
            "@high",
            "@analyse",
            "@coverage"
        ],
        "post-autoload-dump": [],
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/pint --ansi",
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SchenkeIo\\LaravelUrlCleaner\\LaravelUrlCleanerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
